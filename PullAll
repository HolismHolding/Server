. /HolismHolding/Server/Message.sh

CurrentDir=$PWD

for i in $(find . -mindepth 1 -maxdepth 1 -type d -printf '%f\n'); do
    cd $CurrentDir/$i
    . /HolismHolding/Server/Extract
    if [ -f /$Organization/$Repository/docker-compose.yml ]; then
        cd /$Organization/$Repository
        Pull
    fi
done