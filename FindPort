. /HolismHolding/Server/Message.sh

if [ ! -f Ports ]; then
    Error "Ports file is not found in the current directory"
    Error "Run FindPort command in the organization's root directory"
    exit;  
fi

Port=$(cat Ports | grep $Organization$Repository)

Success $Port


